@model DatingProj.Controllers.PostIndexViewModel

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create", new { id = Model.Id })
</p>
<table class="table">
   

@foreach (var item in Model.Posts) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FromUser.UserName): @Html.DisplayFor(modelItem => item.Text)
        </td>
    </tr>
}

</table>

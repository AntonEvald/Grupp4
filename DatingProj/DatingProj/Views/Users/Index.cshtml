@model IEnumerable<DatingProj.Models.ApplicationUser>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
    <tr>
      
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model)
{

    <tr>
       
        <td>
            @Html.DisplayFor(modelItem => item.Name) : @Html.ActionLink("Go to Profil","UserProfil","Users", new { id = item.Id }, new { })
        </td>
    </tr>
}

</table>
